        <td VALIGN="top" align="center"><b><p ALIGN="CENTER"><font size="2">лев</font></b></td>
        <td VALIGN="top" align="center"><b><p ALIGN="CENTER"><font size="2">прав</font></b></td>
        <td VALIGN="top" align="center"><b><p ALIGN="CENTER"><font size="2">лев</font></b></td>
