      <tr>
        <td VALIGN="top" align="center"><p ALIGN="CENTER"><font size="2">70</font></td>
        <td VALIGN="top" align="center"><p ALIGN="CENTER"><font size="2">33.9</font></td>
        <td VALIGN="top" align="center"><p ALIGN="CENTER"><font size="2">34.5</font></td>
        <td VALIGN="top" align="center"><p ALIGN="CENTER"><font size="2">16.8</font></td>
