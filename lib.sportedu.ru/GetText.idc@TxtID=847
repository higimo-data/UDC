        <td WIDTH="11%" VALIGN="TOP"><p ALIGN="CENTER"><font size="2">8.</font></td>
        <td WIDTH="9%" VALIGN="TOP"><p ALIGN="CENTER"><font size="2">3275</font></td>
        <td WIDTH="9%" VALIGN="TOP"><p ALIGN="CENTER"><font size="2">3305</font></td>
