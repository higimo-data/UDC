        <td VALIGN="top" align="center"><p ALIGN="CENTER"><font size="2">2.55,1-3.00,0</font></td>
        <td VALIGN="top" align="center"><p ALIGN="CENTER"><font size="2">2.58,24</font></td>
        <td VALIGN="top" align="center"><p ALIGN="CENTER"><font size="2">40,33</font></td>
