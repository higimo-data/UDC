        <td VALIGN="top"><b><p ALIGN="CENTER"><font size="2">ВК, кг</font></b></td>
        <td VALIGN="top"><b><p ALIGN="CENTER"><font size="2">Рывок</font></b></td>
        <td VALIGN="top"><b><p ALIGN="CENTER"><font size="2">Год установления</font></b></td>
        <td VALIGN="top"><b><p ALIGN="CENTER"><font size="2">Страна</font></b></td>
        <td VALIGN="top"><b><p ALIGN="CENTER"><font size="2">Спортсмен</font></b></td>
      </tr>
      <tr>
        <td VALIGN="top"><p ALIGN="CENTER"><font size="2">52</font></p>
        <p ALIGN="CENTER"><font size="2">56</font></p>
        <p ALIGN="CENTER"><font size="2">60</font></p>
        <p ALIGN="CENTER"><font size="2">67,5</font></p>
        <p ALIGN="CENTER"><font size="2">75</font></p>
        <p ALIGN="CENTER"><font size="2">82,5</font></p>
        <b><p ALIGN="CENTER"><font size="2">90</font></p>
        </b><p ALIGN="CENTER"><font size="2">100</font></p>
        <p ALIGN="CENTER"><font size="2">110</font></p>
        <p ALIGN="CENTER"><font size="2">+110</font></td>
