        <td VALIGN="top" COLSPAN="7" align="center"><p ALIGN="CENTER"><font size="2">0,57</font></td>
        <td VALIGN="top" COLSPAN="6" align="center"><p ALIGN="CENTER"><font size="2">2,62</font></td>
        <td VALIGN="top" COLSPAN="4" align="center"><p ALIGN="CENTER"><font size="2">&lt;0,01</font></td>
      </tr>
      <tr>
        <td VALIGN="top" COLSPAN="22" align="center"><i><p ALIGN="CENTER"><font size="2">Коэффициенты
        уравнения регрессии</font></i></td>
      </tr>
      <tr>
        <td VALIGN="top" COLSPAN="2" align="center"><i><p ALIGN="CENTER"><font size="2">a</font></i></td>
        <td VALIGN="top" COLSPAN="2" align="center"><i><p ALIGN="CENTER"><font size="2">b<sub>1</sub></font></i></td>
        <td VALIGN="top" COLSPAN="3" align="center"><i><p ALIGN="CENTER"><font size="2">b<sub>2</sub></font></i></td>
        <td VALIGN="top" COLSPAN="2" align="center"><i><p ALIGN="CENTER"><font size="2">b<sub>3</sub></font></i></td>
        <td VALIGN="top" COLSPAN="2" align="center"><i><p ALIGN="CENTER"><font size="2">b<sub>4</sub></font></i></td>
        <td VALIGN="top" COLSPAN="3" align="center"><i><p ALIGN="CENTER"><font size="2">b<sub>5</sub></font></i></td>
        <td VALIGN="top" COLSPAN="2" align="center"><i><p ALIGN="CENTER"><font size="2">b<sub>6</sub></font></i></td>
        <td VALIGN="top" COLSPAN="2" align="center"><i><p ALIGN="CENTER"><font size="2">b<sub>7</sub></font></i></td>
        <td VALIGN="top" COLSPAN="2" align="center"><i><p ALIGN="CENTER"><font size="2">b<sub>8</sub></font></i></td>
        <td VALIGN="top" COLSPAN="2" align="center"><i><p ALIGN="CENTER"><font size="2">b<sub>9</sub></font></i></td>
      </tr>
      <tr>
        <td VALIGN="top" COLSPAN="2" align="center"><p ALIGN="CENTER"><font size="2">100,19</font></td>
